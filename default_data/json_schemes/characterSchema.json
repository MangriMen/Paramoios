{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "title": "The Root Schema",
  "description": "The root schema comprises the entire JSON document.",
  "required": [
    "playerName",
    "charName",
    "class",
    "race",
    "alignment",
    "background",
    "level",
    "expirience",
    "hpMax",
    "hp",
    "hpTemp",
    "armorClass",
    "speed",
    "proficiencyBonus",
    "passiveWisdom",
    "inspiration",
    "maxWeight",
    "deathSaves",
    "characteristic",
    "characteristicBonus",
    "savingThrowsBonus",
    "skillsBonus",
    "armament",
    "equipment",
    "money",
    "personality"
