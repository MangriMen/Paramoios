    "playerName": {
      "$id": "#/properties/playerName",
      "type": "string",
      "maxLength": 32,
      "title": "The Playername Schema",
      "description": "A user's name.",
      "default": "",
      "examples": [
        "Bob"
      ]
    },
    "charName": {
      "$id": "#/properties/charName",
      "type": "string",
      "maxLength": 32,
      "title": "The Charname Schema",
      "description": "A name of user's character.",
      "default": "",
      "examples": [
        "Bruenor"
      ]
    },
    "class": {
      "$id": "#/properties/class",
      "type": "string",
      "maxLength": 32,
      "title": "The Class Schema",
      "description": "User's character's main job or profession.",
      "default": "",
      "examples": [
        "barbarian"
      ]
    },
    "race": {
      "$id": "#/properties/race",
      "type": "string",
      "maxLength": 32,
      "title": "The Race Schema",
